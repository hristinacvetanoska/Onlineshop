@model int
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
<td style="width: 150px">
    <div class="btn-group">
        <a asp-action="Edit" class="btn btn-primary" asp-route-id="@Model">
        <i style='font-size:24px' class='far'>&#xf044;</i>
        </a>
        <a asp-action="Details" class="btn btn-success" asp-route-id="@Model">
            <i style='font-size:24px' class='fas'>&#xf0ca;</i>
        </a>
        <a asp-action="Delete" class="btn btn-danger" asp-route-id="@Model">
            <i style='font-size:24px' class='fas'>&#xf2ed;</i>
        </a>
    </div>
</td>
